#Change line 5 and 8 to make your own passwords

password=None
attempts=[]
while password !="YOUR PASSWORD":
    password = input("Enter the password:")
    attempts.append(password)
    if password !="YOUR PASSWORD":
        print("Your password is inccorrect.You have already guessed")
        for i in attempts:
            print(i)
print("Correct password.Welcome")    
